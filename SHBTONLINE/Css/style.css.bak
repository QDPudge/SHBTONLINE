/*----------------------------------
		Typhography
----------------------------------*/
h1,h2,h3,h4,h5,h6,p,a{
	margin: 0px;
	padding: 0px;
}
ul,ol{
	margin: 0px;
	padding: 0px;
	list-style: none;
}
a,a:focus,a:hover{
	text-decoration: none;
}
a:focus{
	outline: none;
}
h1{
	color: #222;
	font-family: 'Lato-Black';
	font-size: 60px;
}
h2{
	color: #222;
	font-size: 30px;
	font-family: 'Lato-Black';
}
h3{
	color: #222;
	font-family: 'Lato',sans-srif;
	font-weight: 400;
	font-size: 28px;
}
h4{
	font-size: 26px;
}
h6{
	color: #222;
	font-family: 'Lato-Heavy';
	font-size: 14px;
}
p{
	color: #aaa;
	font-family: 'Lato',sans-serif;
	font-weight: 400;
	font-size: 14px;
}

